<!-- SUNVISORS ############################## -->	
    <Template Name="G36mod_Sun_Visors">
      <DefaultTemplateParameters>
        <USE_TRAJECTORY_DRAG_MODE>True</USE_TRAJECTORY_DRAG_MODE>
        <SCROLL_INCREMENT>701</SCROLL_INCREMENT>
      </DefaultTemplateParameters>
      <Component ID="#ID#" Node="#NODE_ID#">
        <UseTemplate Name="ASOBO_GT_Lever_DraggingWheelLeftDrag_Axis">
          <CURSOR>Grab</CURSOR>
          <NO_ARROWS/>
          <DRAG_CODE>
            16384 (M:DragPercent) *
            16384 min 0 max (>L:XMLVAR_G36mod_sun_visor_#ID#)
          </DRAG_CODE>
          <ANIM_CODE>(L:XMLVAR_G36mod_sun_visor_#ID#, position 16k) 16384 / 100 *</ANIM_CODE>
          <ANIM_NAME>G36mod_sun_visor_#ID#</ANIM_NAME>
          <DRAG_ANIM_NAME>G36mod_sun_visor_#ID#</DRAG_ANIM_NAME>
          <POSITIVE_AXIS_CODE>(L:XMLVAR_G36mod_sun_visor_#ID#, position 16k) #SCROLL_INCREMENT# + 16384 min (>L:XMLVAR_G36mod_sun_visor_#ID#)</POSITIVE_AXIS_CODE>
          <NEGATIVE_AXIS_CODE>(L:XMLVAR_G36mod_sun_visor_#ID#, position 16k) #SCROLL_INCREMENT# - 0 max (>L:XMLVAR_G36mod_sun_visor_#ID#)</NEGATIVE_AXIS_CODE>
          <TOOLTIPID>Sun Visor</TOOLTIPID>
          <WWISE_EVENT_1>cabin_heat_lever_on</WWISE_EVENT_1>
          <WWISE_EVENT_2>audio_switch_intercom</WWISE_EVENT_2> 
        </UseTemplate>
      </Component>
    </Template>
